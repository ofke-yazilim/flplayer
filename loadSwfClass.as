class loadSwfClass extends flash.external.ExternalInterface{    public var container_mc_in_class:MovieClip;	public var mclListener:Object  = new Object();	public var counter:Number;	public var difference:Number   = 0;		public var clickSwf:Number     = 1;    var checkOnload:Number         = 0;		function loadSwfClass(){		//Stop/Play tuslari calistiriliyor.		addCallback("swfStopPlay", this , swfStopPlay);				//Istedigimiz frameden oynatmaya basliyoruz.		addCallback("goToFrame", this , goToFrame);				//Istedigimiz frameden oynatmayi durdurur		addCallback("goToFrameAndStop", this , goToFrameAndStop);		    }						//playStatus 1 ise ders oynatilacak.	function loadSwf(s3Link,bucketName,slash,swfName,container_mc:MovieClip,counter,playStatus){		var mouseListener:Object = new Object();		mouseListener.onMouseDown = function() {			/*if(clickSwf == 0){				clickSwf = 1;				_level0.container_mc.stop();			} else{				clickSwf = 0;				_level0.container_mc.play();			}*/		};				mouseListener.onMouseMove = function() {			};				mouseListener.onMouseUp = function() {			};		Mouse.addListener(mouseListener);		//Swf dosyasi yüklenirken olusabilecek hatalari takip ediyoruz.		mclListener.onLoadError = function(target_mc:MovieClip, errorCode:String,status:Number) {			trace("Error: " + errorCode + " [" + status + "]");		};				//Swf container uzerine yuklenmeye baslandi		mclListener.onLoadStart = function(target_mc:MovieClip):Void {			trace("onLoadStart: " + target_mc);		};				//onLoadProgress eventi ile swf dosyamizin yüklenme asamalarini takip ediyoruz.		mclListener.onLoadProgress = function(target_mc:MovieClip,numBytesLoaded:Number, numBytesTotal:Number):Void {			var numPercentLoaded:Number = numBytesLoaded / numBytesTotal * 100;			var stopFirstFrame:Number   = 0;						target_mc.onEnterFrame = function()  { counter++;							if(counter==1 && playStatus==0){					flash.external.ExternalInterface.call("alert","faruk faruk");					_level0.container_mc.gotoAndStop(1);					_level0.container_mc.stop();				}								//Oynayan slide ile total slide arasindaki fark aliniyor			    difference = _level0.container_mc._totalframes-_level0.container_mc._currentframe;							if(_level0.container_mc._currentframe % 10 == 0){				   //Oynayan mevcut frame bilgisini html javascript icerisine doner				   flash.external.ExternalInterface.call("getCurrentFrame",_level0.container_mc._currentframe);				}								if(difference<10 && _level0.container_mc._currentframe == _level0.container_mc._totalframes){				   //Oynayan mevcut frame bilgisini html javascript icerisine doner				   flash.external.ExternalInterface.call("getCurrentFrame",_level0.container_mc._currentframe);				}			   			    if(this._currentframe==_level0.container_mc._totalframes){				   //Swf total frame sayisina ulasinca durduruluyor				   _level0.container_mc.gotoAndStop(1);			    }			}		};				//On tanimlamalar yapiliyor		mclListener.onLoadInit = function(target_mc:MovieClip) {			/*target_mc._width  = 550;			target_mc._height = 401;*/						 target_mc._xscale = 83;             target_mc._yscale = 73;						//flash.external.ExternalInterface.call("alert","girdi-2");						target_mc.scaleX = 0;			target_mc.scaleY = 0;						target_mc._x = 0;			target_mc._y = 0;								};				//Swf yukleme islemi tamamlandi		mclListener.onLoadComplete = function(target_mc:MovieClip,status:Number,clickSwf):Void {			trace("onLoadComplete: " + target_mc);			flash.external.ExternalInterface.call("getTotalFrame",_level0.container_mc._totalframes);		};		        var mcl:MovieClipLoader = new MovieClipLoader();		mcl.addListener(mclListener);		mcl.loadClip(bucketName+swfName,container_mc);	}	    //Ilgili frame gidiliyor ve oynamaya devam ediyor	function goToFrame(frame:Number){		clickSwf = 0;		_level0.container_mc.gotoAndPlay(frame);	}	    //Ilgili frame gidiliyor ve duruyor	function goToFrameAndStop(frame:Number){		clickSwf = 1;		_level0.container_mc.gotoAndStop(frame);	}		//Tiklandigi anda oynayan swf durur ya da durmuş swf oynar	function swfStopPlay(container_mc:MovieClip){		if(clickSwf == 0){			clickSwf = 1;			_level0.container_mc.stop();		} else{			clickSwf = 0;			_level0.container_mc.play();		}	}		//Click swf value ayalaniyor	//Click Swf degeri döndürülüyor	function getClickSwf():Number {		return clickSwf;	}		//Clickswf degeri set ediliyor	function setClickSwf(clickSwf:Number):Void {		this.clickSwf = clickSwf;	}}